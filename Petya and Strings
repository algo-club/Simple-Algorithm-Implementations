#include <bits/stdc++.h>
using namespace std;
int main(){
    string h,k;
    cin>>h>>k;
    for(int i=0;i<h.length();i++){
       h[i] = tolower(h[i]);
    }
    for(int i=0;i<k.length();i++){
       k[i] = tolower(k[i]);
    }
    if(h==k){cout<<0<<endl;}
    else if(h>k){cout<<1<<endl;}
    else{cout<<-1<<endl;}
    return 0;
}
